{
  "ipv4-addr": {
    "fields": {
      "value": [
        "sourceaddress",
        "destinationaddress",
        "ipaddress"
      ]
    }
  },
  "network-traffic": {
    "fields": {
      "src_port": ["sourceport"],
      "dst_port": ["destinationport"],
      "protocols[*]": ["transportprotocol"],
      "start": ["eventtime"],
      "end": ["eventtime"]
    }
  },
  "process": {
    "fields": {
      "name": [
        "processname",
        "sourceprocessname",
        "destinationprocessname"
      ],
      "pid": ["processid"],
      "command_line": ["commandline"],
      "parent_ref.pid": ["parentprocessid"]
    }
  },
  "x-oca-event": {
    "fields": {
      "action": ["deviceaction"],
      "outcome": ["categoryoutcome"],
      "category": ["category"],
      "domain": ["domain"],
      "hostname": [
        "devicehostname",
        "hostname"
      ]
    }
  },
  "user-account": {
    "fields": {
      "user_id": [
        "username",
        "accountname"
      ],
      "display_name": ["username"]
    }
  }
}